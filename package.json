{
  "name": "yuits",
  "version": "1.0.0",
  "description": "YuiBot TypeScript version",
  "main": "yui/yui.ts",
  "scripts": {
    "prebuild-win": "IF EXIST .\\dist rmdir /Q /S .\\dist && mkdir dist",
    "prebuild": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run prebuild && export NODE_ENV=build && tsc -p tsconfig.build.json && node dist/yui.js",
    "dev": "npm run prebuild && export NODE_ENV=development && tsc -p tsconfig.build.json && node dist/yui.js",
    "build": "npm run prebuild && tsc -p tsconfig.json",
    "build:docker": "tsc -p tsconfig.build.json",
    "start:dev": "export NODE_ENV=development && tsc-watch --onSuccess \"node dist/yui.js\"",
    "start:build": "export NODE_ENV=build && tsc-watch --onSuccess \"node dist/yui.js\"",
    "start:win": "npm run prebuild-win && set NODE_ENV=build && tsc -p tsconfig.json && node dist/yui.js",
    "cpyfiles": "copyfiles --up 2 --all yui/assets/**/* dist/assets",
    "lint": "eslint '*/**/*.{ts,tsx}' --quiet --fix"
  },
  "dependencies": {
    "@discordjs/collection": "^0.3.2",
    "@discordjs/opus": "^0.6.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "@discordjs/voice": "^0.7.4",
    "axios": "^0.24.0",
    "bili-api": "^1.7.1",
    "bufferutil": "^4.0.3",
    "bull": "^3.26.0",
    "chalk": "^4.1.0",
    "copyfiles": "^2.4.1",
    "discord-api-types": "^0.24.0",
    "discord.js": "^13.3.1",
    "dotenv": "^8.2.0",
    "erlpack": "github:discordapp/erlpack",
    "ffmpeg-static": "^4.4.0",
    "fs": "^0.0.1-security",
    "googleapis": "^81.0.0",
    "ioredis": "^4.27.6",
    "lodash": "^4.17.21",
    "log4js": "^6.1.2",
    "m3u8stream": "^0.7.1",
    "module-alias": "^2.2.2",
    "opusscript": "0.0.7",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "sodium": "^3.0.2",
    "tslib": "^1.14.1",
    "tweetnacl": "^1.0.3",
    "utf-8-validate": "^5.0.4",
    "winston": "^3.3.3",
    "ytdl-core": "^4.10.1",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@mapbox/node-pre-gyp": "^1.0.6",
    "@types/bull": "^3.15.2",
    "@types/ioredis": "^4.26.5",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "eslint": "^7.19.0",
    "node-gyp-build": "^4.3.0",
    "patch-package": "^6.2.2",
    "tsc-watch": "^4.5.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
