{
  "name": "yuits",
  "version": "1.1.0",
  "description": "YuiBot TypeScript version",
  "main": "yui/yui.ts",
  "scripts": {
    "prebuild-win": "IF EXIST .\\dist rmdir /Q /S .\\dist && mkdir dist",
    "prebuild": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run prebuild && tsc -p tsconfig.json",
    "start": "npm run build && export NODE_ENV=build && node dist/yui.js",
    "dev": "npm run build && export NODE_ENV=development && node dist/yui.js",
    "build:docker": "tsc -p tsconfig.build.json",
    "start:dev": "export NODE_ENV=development && tsc-watch --onSuccess \"node ./dist/yui.js\"",
    "start:win": "npm run prebuild-win && set NODE_ENV=build && tsc -p tsconfig.json && node dist/yui.js",
    "lint": "eslint '*/**/*.ts' --quiet --fix",
    "//": "===============================================================================================",
    "//0": "Build lightning using swc (https://swc.rs/)",
    "//1": "But there's a drawback that you would need nodemon and concurrently to pull this off",
    "swc:build": "npm run prebuild && npx swc ./yui -d dist --only **/*.ts",
    "swc:start": "export NODE_ENV=build && node ./dist/yui.js",
    "swc:start-dev": "export NODE_ENV=development && node ./dist/yui.js",
    "swc:watch-dev": "swc ./yui -d dist --only **/*.ts -w --log-watch-compilation",
    "swc:nodemon": "export NODE_ENV=development && nodemon --delay 200ms --watch dist dist/yui.js",
    "swc:dev-cc": "concurrently -n swc,nodemon -k \"npm run swc:watch-dev\" \"npm run swc:nodemon\""
  },
  "dependencies": {
    "@discordjs/collection": "^0.5.0",
    "@discordjs/opus": "^0.7.0",
    "@discordjs/rest": "^0.3.0",
    "@discordjs/voice": "^0.8.0",
    "axios": "^0.26.1",
    "bili-api": "^1.7.2",
    "bufferutil": "^4.0.6",
    "chalk": "^4.1.0",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "erlpack": "^0.1.4",
    "ffmpeg-static": "^5.0.0",
    "googleapis": "^100.0.0",
    "ioredis": "^5.0.3",
    "lodash": "^4.17.21",
    "m3u8stream": "^0.8.6",
    "module-alias": "^2.2.2",
    "opusscript": "^0.0.8",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "sodium": "^3.0.2",
    "tslib": "^2.3.1",
    "tweetnacl": "^1.0.3",
    "utf-8-validate": "^5.0.9",
    "uuid": "^8.3.2",
    "winston": "^3.7.2",
    "ytdl-core": "^4.11.0",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@mapbox/node-pre-gyp": "^1.0.9",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.163",
    "@swc/helpers": "^0.3.8",
    "@types/ioredis": "^4.28.10",
    "@types/lodash": "^4.14.181",
    "@types/node": "^17.0.23",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "chokidar": "^3.5.3",
    "concurrently": "^7.1.0",
    "eslint": "^8.12.0",
    "node-gyp-build": "^4.4.0",
    "nodemon": "^2.0.15",
    "patch-package": "^6.4.7",
    "tsc-watch": "^5.0.2",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3",
    "yarn-upgrade-all": "^0.7.1"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
