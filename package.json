{
  "name": "yuits",
  "version": "1.1.0",
  "description": "YuiBot TypeScript version",
  "main": "yui/yui.ts",
  "scripts": {
    "postinstall": "npm link djs-ioc-container",
    "//0": "Build lightning fast using swc (https://swc.rs/)",
    "//1": "But there's a drawback that you would need nodemon (https://www.npmjs.com/package/nodemon) and concurrently (https://www.npmjs.com/package/concurrently) to pull this off seamlessly",
    "swc:build": "npm run prebuild && swc ./yui -d dist --only **/*.ts",
    "swc:start": "export NODE_ENV=build && node -r tsconfig-paths/register dist/yui.js",
    "swc:start:dev": "export NODE_ENV=development && node -r tsconfig-paths/register dist/yui.js",
    "swc:watch:dev": "swc yui -d dist --only **/*.ts -w --log-watch-compilation",
    "swc:dev": "concurrently --handle-input -n swc,nodemon -k \"npm run swc:watch:dev\" \"npm run nodemon:dev\"",
    "swc:node": "NODE_ENV=development node -r @swc-node/register yui/yui.ts",
    "//2": "Build using notmal typescript (tsc)",
    "nodemon:dev": "export NODE_ENV=development && nodemon --delay 200ms --watch dist dist/yui.js",
    "prebuild": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run prebuild && tsc -p tsconfig.json",
    "start": "npm run build && export NODE_ENV=build && node -r tsconfig-paths/register dist/yui.js",
    "dev": "npm run build && export NODE_ENV=development && node -r tsconfig-paths/register dist/yui.js",
    "build:docker": "tsc -p tsconfig.json",
    "lint": "eslint '*/**/*.ts' --quiet --ext .ts --fix ./yui",
    "pretty": "prettier --config .prettierrc --write ./yui"
  },
  "dependencies": {
    "@discordjs/collection": "^1.1.0",
    "@discordjs/opus": "^0.8.0",
    "@discordjs/rest": "^1.2.0",
    "@discordjs/voice": "^0.11.0",
    "@tlp01/djs-ioc-container": "^1.0.2",
    "axios": "^0.26.1",
    "bufferutil": "^4.0.6",
    "chalk": "^4.1.0",
    "discord.js": "^14.6.0",
    "dotenv": "^16.0.0",
    "erlpack": "^0.1.4",
    "ffmpeg-static": "^5.0.0",
    "googleapis": "^100.0.0",
    "i": "^0.3.7",
    "ioredis": "^5.0.3",
    "lodash": "^4.17.21",
    "m3u8stream": "^0.8.6",
    "module-alias": "^2.2.2",
    "npm": "^8.19.2",
    "opusscript": "^0.0.8",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "semver": "^7.3.7",
    "sodium": "^3.0.2",
    "tsconfig-paths": "^4.1.0",
    "utf-8-validate": "^5.0.9",
    "winston": "^3.7.2",
    "ytdl-core": "^4.11.2"
  },
  "devDependencies": {
    "@mapbox/node-pre-gyp": "^1.0.9",
    "@swc-node/register": "^1.5.1",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.3",
    "@swc/helpers": "^0.4.11",
    "@types/ioredis": "^4.28.10",
    "@types/lodash": "^4.14.181",
    "@types/node": "^17.0.23",
    "@types/semver": "^7.3.10",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "concurrently": "^7.1.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-unicorn": "^43.0.2",
    "node-gyp-build": "^4.4.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "prettier-eslint": "^15.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.3",
    "yarn-upgrade-all": "^0.7.1"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}