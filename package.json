{
  "name": "yuits",
  "version": "1.0.0",
  "description": "YuiBot TypeScript version",
  "main": "yui/yui.ts",
  "scripts": {
    "prebuild-win": "IF EXIST .\\dist rmdir /Q /S .\\dist && mkdir dist",
    "prebuild": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run prebuild && tsc -p tsconfig.json",
    "start": "npm run build && export NODE_ENV=build && node dist/yui.js",
    "dev": "npm run build && export NODE_ENV=development && node dist/yui.js",
    "build:docker": "tsc -p tsconfig.build.json",
    "start:dev": "export NODE_ENV=development && tsc-watch --onSuccess \"node dist/yui.js\"",
    "start:win": "npm run prebuild-win && set NODE_ENV=build && tsc -p tsconfig.json && node dist/yui.js",
    "lint": "eslint '*/**/*.ts' --quiet --fix",
    "//": "===============================================================================================",
    "//0": "Build lightning using swc (https://swc.rs/)",
    "//1": "But there's a drawback that you would need nodemon and concurrently to pull this off",
    "swc:build": "npm run prebuild && npx swc ./yui -d dist --only **/*.ts",
    "swc:start": "export NODE_ENV=build && node ./dist/yui.js",
    "swc:start-dev": "export NODE_ENV=development && node ./dist/yui.js",
    "swc:watch-dev": "swc ./yui -d dist --only **/*.ts -w --log-watch-compilation",
    "swc:nodemon": "export NODE_ENV=development && nodemon --delay 200ms --watch dist dist/yui.js",
    "swc:dev-cc": "concurrently -n swc,nodemon -k \"npm run swc:watch-dev\" \"npm run swc:nodemon\""
  },
  "dependencies": {
    "@discordjs/collection": "^0.3.2",
    "@discordjs/opus": "^0.6.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "@discordjs/voice": "^0.7.4",
    "axios": "^0.24.0",
    "bili-api": "^1.7.1",
    "bufferutil": "^4.0.3",
    "bull": "^3.26.0",
    "chalk": "^4.1.0",
    "discord-api-types": "^0.24.0",
    "discord.js": "^13.3.1",
    "dotenv": "^8.2.0",
    "erlpack": "github:discordapp/erlpack",
    "ffmpeg-static": "^4.4.0",
    "fs": "^0.0.1-security",
    "googleapis": "^81.0.0",
    "ioredis": "^4.27.6",
    "lodash": "^4.17.21",
    "log4js": "^6.4.0",
    "m3u8stream": "^0.7.1",
    "module-alias": "^2.2.2",
    "opusscript": "0.0.7",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "sodium": "^3.0.2",
    "tslib": "^1.14.1",
    "tweetnacl": "^1.0.3",
    "utf-8-validate": "^5.0.4",
    "winston": "^3.3.3",
    "ytdl-core": "^4.11.0",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@mapbox/node-pre-gyp": "^1.0.6",
    "@swc/cli": "^0.1.56",
    "@swc/core": "^1.2.160",
    "@swc/helpers": "^0.3.8",
    "@types/bull": "^3.15.2",
    "@types/ioredis": "^4.26.5",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "chokidar": "^3.5.3",
    "concurrently": "^7.0.0",
    "eslint": "^8.12.0",
    "node-gyp-build": "^4.3.0",
    "nodemon": "^2.0.15",
    "patch-package": "^6.2.2",
    "tsc-watch": "^4.5.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
